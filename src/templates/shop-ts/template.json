{
  "name": "shop-template",
  "version": "1.0.0",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "ts-loader": "^9.0.0",
    "typescript": "^4.5.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.1.1",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.0.0",
    "@testing-library/react": "^13.0.0"
  },
  "scripts": {
    "start": "webpack serve --open",
    "build": "webpack",
    "postinstall": "npm run build",
    "test": "jest"
  },
  "routes": [
    {
      "path": "/",
      "component": "Home"
    },
    {
      "path": "/product/:id",
      "component": "product"
    }
  ],
  "features": {
    "ecommerce": true,
    "authentication": false
  }
}